# /server/.env.example
# Server
PORT=5000
NODE_ENV=production
FRONTEND_URL=https://your-elastic-beanstalk-url.elasticbeanstalk.com

# Database
DB_HOST=your-rds-endpoint.rds.amazonaws.com
DB_PORT=5432
DB_NAME=taskmanager
DB_USER=postgres
DB_PASSWORD=your_strong_password

# JWT
JWT_SECRET=your_very_strong_jwt_secret_key

# AWS
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-east-1
AWS_S3_BUCKET_NAME=your-task-manager-bucket